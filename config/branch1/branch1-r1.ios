 network 10.11.12.88 255.255.255.248
 default-router 10.11.12.89
exit


ipv6 unicast-routing

ipv6 dhcp pool V10-Branch1-HR
 address prefix 2000:12:34:20::/64
 dns-server 2000:12:34:0::8888
 domain-name cisco.com


ipv6 dhcp pool V20-Branch1-NS
 address prefix 2000:12:34:21::/64
 dns-server 2000:12:34:0::8888
 domain-name cisco.com


ipv6 dhcp pool V30-Branch1-PR
